{
	"name": "reactive",
	"version": "0.1.0",
	"private": true,
	"homepage": "https://ownithomeloans.com.au/wp-content/themes/reactive",
	"dependencies": {
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.5.0",
		"@testing-library/user-event": "^7.2.1",
		"@types/jest": "^24.9.1",
		"@types/node": "^12.12.47",
		"@types/react": "^16.9.38",
		"@types/react-dom": "^16.9.8",
		"axios": "^0.19.2",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-scripts": "3.4.1",
		"styled-components": "^5.1.1",
		"typescript": "^3.7.5"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "npm run delete-build && react-scripts build && npm run convert-index-to-php && npm run copy-theme-files",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"delete-build": "rimraf -rf build",
		"convert-index-to-php": "mv build/index.html build/index.php",
		"copy-theme-files": "cp wp-theme/* build"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@types/styled-components": "^5.1.0",
		"@types/styled-theming": "^2.2.3",
		"@typescript-eslint/eslint-plugin": "^3.3.0",
		"@typescript-eslint/parser": "^3.3.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.20.0",
		"prettier": "^2.0.5",
		"rimraf": "^3.0.2"
	}
}
